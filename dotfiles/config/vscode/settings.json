{
  "window.titleBarStyle": "custom",
  "window.title": "${rootPath}${separator}${appName}",
  "window.zoomLevel": 2,
  "window.menuBarVisibility": "toggle",
  "sync.autoDownload": true,
  "sync.autoUpload": true,
  "sync.forceDownload": true,
  "sync.forceUpload": true,
  "sync.quietSync": true,
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.enablePreview": false,
  "workbench.editor.showTabs": "multiple",
  "workbench.editor.pinnedTabSizing": "shrink",
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.centeredLayoutAutoResize": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.settings.editor": "json",
  "workbench.colorTheme": "Dracula Soft",
  "workbench.startupEditor": "none",
  "workbench.colorCustomizations": {
    "statusBar.background": "#00000077",
    "editor.lineHighlightBackground": "#00000033",
    "editor.lineHighlightBorder": "#aaaaaa33",
    "editor.selectionBackground": "#222"
  },
  "editor.rename.enablePreview": false,
  "editor.fontSize": 17,
  "editor.fontFamily": "'Droid Sans Mono'",
  "editor.fontLigatures": true,
  "editor.lineHeight": 22,
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 80,
  "editor.wrappingStrategy": "advanced",
  "editor.formatOnSave": false,
  "editor.formatOnPaste": false,
  "editor.formatOnType": false,
  "editor.inlineSuggest.enabled": true,
  "editor.tabSize": 2,
  "editor.scrollBeyondLastLine": false,
  "editor.cursorBlinking": "phase",
  "editor.linkedEditing": true,
  "editor.minimap.enabled": true,
  "editor.suggestSelection": "first",
  "editor.selectionClipboard": false,
  "editor.renderWhitespace": "boundary",
  "editor.unicodeHighlight.invisibleCharacters": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.inlayHints.enabled": "on",
  "editor.codeActionsOnSave": {
    //"source.fixAll.shellcheck": "explicit",
    // "source.fixAll.eslint": "explicit",
    // "source.addMissingImports": "explicit"
  },
  "editor.quickSuggestions": {
    "comments": true,
    "strings": true
  },
  "files.autoSave": "onFocusChange",
  "files.insertFinalNewline": false,
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "**/node_modules/*/**": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true
  },
  "files.associations": {
    "*.js": "javascriptreact",
    "*.ts": "typescriptreact"
  },
  "formatFiles.excludedFolders": [
    "node_modules",
    ".vscode",
    ".git",
    "dist",
    ".chrome"
  ],
  "explorer.autoReveal": true,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "explorer.openEditors.visible": 1,
  "terminal.explorerKind": "external",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.fontFamily": "Fira Code,NerdFontsSymbolsOnly,Hack,JetBrains Mono",
  "terminal.integrated.shellIntegration.enabled" : false,
  "terminal.integrated.gpuAcceleration": "off",
  "extensions.ignoreRecommendations": true,
  "vsicons.dontShowNewVersionMessage": true,
  "diffEditor.ignoreTrimWhitespace": true,
  "diffEditor.renderSideBySide": true,
  "cSpell.language": "en-GB",
  "cSpell.enabled": true,
  "cSpell.checkLimit": 1000,
  "cSpell.minWordLength": 5,
  "cSpell.allowCompoundWords": true,
  "cSpell.diagnosticLevel": "Information",
  "cSpell.enableFiletypes": [
    "markdown",
    "!json",
    "!go"
  ],
  "cSpell.checkOnlyEnabledFileTypes": true,
  "cSpell.allowedSchemas": [
    "file",
    "gist",
    "repo",
    "sftp",
    "untitled",
    "vscode-notebook-cell",
    "vscode-scm",
    "vscode-userdata"
