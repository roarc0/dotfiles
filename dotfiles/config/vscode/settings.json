{
  "window.titleBarStyle": "custom",
  "window.title": "${rootPath}${separator}${appName}",
  "window.zoomLevel": 1,
  "window.menuBarVisibility": "toggle",
  "sync.autoDownload": true,
  "sync.autoUpload": true,
  "sync.forceDownload": true,
  "sync.forceUpload": true,
  "sync.quietSync": true,
  "workbench.editor.enablePreview": false,
  "workbench.editor.showTabs": "multiple",
  "workbench.editor.pinnedTabSizing": "shrink",
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.untitled.hint": "hidden",
  "workbench.editor.centeredLayoutAutoResize": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.settings.editor": "json",
  "workbench.iconTheme": "vscode-icons",
  "workbench.colorTheme": "One Monokai",
  "workbench.startupEditor": "none",
  "workbench.colorCustomizations": {
    "statusBar.background": "#00000077",
    "editor.lineHighlightBackground": "#00000033",
    "editor.lineHighlightBorder": "#aaaaaa33",
    "editor.selectionBackground": "#222",
  },
  "editor.rename.enablePreview": false,
  "editor.fontSize": 17,
  "editor.fontFamily": "'Droid Sans Mono'",
  "editor.fontLigatures": true,
  "editor.lineHeight": 22,
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 80,
  "editor.wrappingStrategy": "advanced",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": false,
  "editor.inlineSuggest.enabled": true,
  "editor.tabSize": 2,
  "editor.scrollBeyondLastLine": false,
  "editor.cursorBlinking": "phase",
  "editor.linkedEditing": true,
  "editor.minimap.enabled": true,
  "editor.suggestSelection": "first",
  "editor.selectionClipboard": false,
  "editor.renderWhitespace": "boundary",
  "editor.unicodeHighlight.invisibleCharacters": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.inlayHints.enabled": "on",
  "editor.codeActionsOnSave": {
    "source.fixAll.shellcheck": true,
    "source.fixAll.eslint": true,
    "source.addMissingImports": true
  },
  "editor.quickSuggestions": {
    "others": true,
    "comments": true,
    "strings": true
  },
  "files.autoSave": "onFocusChange",
  "files.insertFinalNewline": false,
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "**/node_modules/*/**": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true
  },
  "files.associations": {
    "*.js": "javascriptreact",
    "*.ts": "typescriptreact"
  },
  "formatFiles.excludedFolders": [
    "node_modules",
    ".vscode",
    ".git",
    "dist",
    ".chrome"
  ],
  "explorer.autoReveal": true,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "explorer.openEditors.visible": 1,
  "terminal.explorerKind": "external",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.enableShellIntegration": true,
  "terminal.integrated.rendererType": "dom",
  "extensions.ignoreRecommendations": true,
  "vsicons.dontShowNewVersionMessage": true,
  "diffEditor.ignoreTrimWhitespace": true,
  "diffEditor.renderSideBySide": true,
  "cSpell.userWords": [
    "bgcolor",
    "Debugf",
    "fsnotify",
    "grpc",
    "Infof",
    "mipmaps",
    "Plotly",
    "Tracef",
    "Warnf"
  ],
  "liveServer.settings.donotVerifyTags": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "C_Cpp.intelliSenseEngine": "disabled",
  "C_Cpp.default.cppStandard": "c++17",
  "C_Cpp.default.cStandard": "c11",
  "C_Cpp.clang_format_path": "/usr/bin/clang-format",
  "C_Cpp.addNodeAddonIncludePaths": true,
  "C_Cpp.autocompleteAddParentheses": true,
  "C_Cpp.clang_format_sortIncludes": true,
  "clangd.onConfigChanged": "restart",
  "clangd.path": "/home/roarc/.config/Code/User/globalStorage/llvm-vs-code-extensions.vscode-clangd/install/16.0.2/clangd_16.0.2/bin/clangd",
  "clangd.restartAfterCrash": true,
  "clangd.checkUpdates": true,
  "[cpp]": {
    "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
  },
  "cmake.configureOnOpen": false,
  "go.inferGopath": false,
  "go.useLanguageServer": true,
  "go.formatTool": "gofmt",
  "go.lintTool": "golint",
  "go.lintOnSave": "workspace",
  "go.vetOnSave": "workspace",
  "go.toolsManagement.autoUpdate": true,
  "go.coverOnTestPackage": false,
  "[go.mod]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "go.delveConfig": {
    "dlvLoadConfig": {
      "followPointers": true,
      "maxVariableRecurse": 5,
      "maxStringLen": 1024,
      "maxArrayValues": 1024,
      "maxStructFields": -1
    },
    "apiVersion": 2,
    "showGlobalVariables": false,
  },
  "go.languageServerExperimentalFeatures": {
    "diagnostics": true
  },
  "go.testFlags": [
    "-v"
  ],
  "gopls": {
    "usePlaceholders": true,
    "staticcheck": true,
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
  },
  "rust-analyzer.check.command": "clippy",
  "rust-analyzer.check.features": "all",
  "rust-analyzer.semanticHighlighting.strings.enable": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "html.format.enable": true,
  "html.format.indentHandlebars": true,
  "html.format.endWithNewline": false,
  "css.lint.important": "warning",
  "stylelint.enable": true,
  "scss.lint.important": "warning",
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.validate": true,
  "javascript.format.enable": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "javascript.format.semicolons": "remove",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.preferences.includePackageJsonAutoImports": "on",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.format.semicolons": "remove",
  "typescript.format.enable": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.useESLintClass": true,
  "eslint.lintTask.enable": true,
  "eslint.validate": [
    "typescript"
  ],
  "emmet.triggerExpansionOnTab": true,
  "emmet.includeLanguages": {
    "postcss": "css",
    "javascript": "javascriptreact",
    "typescript": "typescriptreact",
    "vue": "html",
    "markdown": "markdown",
    "html": "html"
  },
  "emmet.syntaxProfiles": {
    "postcss": "css"
  },
  "markdown.preview.scrollEditorWithPreview": true,
  "markdown.preview.scrollPreviewWithEditor": true,
  "python.languageServer": "Default",
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.formatOnType": true
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "vs-kubernetes": {
    "vscode-kubernetes.helm-path.linux": "/home/roarc/.local/state/vs-kubernetes/tools/helm/linux-amd64/helm",
    "vscode-kubernetes.kubectl-path.linux": "/home/roarc/.local/state/vs-kubernetes/tools/kubectl/kubectl",
    "vscode-kubernetes.minikube-path.linux": "/home/roarc/.local/state/vs-kubernetes/tools/minikube/linux-amd64/minikube",
    "vs-kubernetes.minikube-show-information-expiration": "2024-01-30T07:26:38.769Z"
  },
  "cSpell.language": "en-GB",
  "cSpell.enabled": true,
  "cSpell.checkLimit": 70000,
  "cSpell.minWordLength": 4,
  "cSpell.allowCompoundWords": true,
  "cSpell.diagnosticLevel": "Information",
  "shellcheck.run": "onType",
  "shellcheck.exclude": [
    "2148",
    "1091",
    "1090"
  ],
  "git.openRepositoryInParentFolders": "never",
  "git.confirmSync": false,
  "git.autofetch": true,
  "github.copilot.enable": {
    "*": false,
    "yaml": false,
    "plaintext": true,
    "markdown": false
  },
  "gitlab.aiAssistedCodeSuggestions.enabled": true,
  "emojisense.languages": {
    "markdown": true,
    "json": true,
    "plaintext": {
      "markupCompletionsEnabled": false,
      "emojiDecoratorsEnabled": false
    },
    "scminput": true
  },
  "deno.enable": false,
  "npm.keybindingsChangedWarningShown": true,
  "svelte.enable-ts-plugin": true,
