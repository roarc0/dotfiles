  "markdown.preview.scrollPreviewWithEditor": true,
  "markdown-preview-enhanced.plantumlJarPath": "/home/roarc/.local/jar/plantuml.jar",
  "python.languageServer": "Default",
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.formatOnType": true
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "vs-kubernetes": {
    "vscode-kubernetes.helm-path.linux": "/home/roarc/.local/bin/helm",
    "vscode-kubernetes.kubectl-path.linux": "/home/roarc/.local/bin/kubectl",
    "vscode-kubernetes.minikube-path.linux": "/home/roarc/.local/bin/minikube",
    "vs-kubernetes.crd-code-completion": "disabled"
  },
  "shellcheck.run": "onSave",
  "shellcheck.exclude": [
    "2148",
    "1091",
    "1090"
  ],
  "git.openRepositoryInParentFolders": "never",
  "git.confirmSync": false,
  "git.autofetch": true,
  "github.copilot.enable": {
    "*": true,
    "sql": true,
    "yaml": true,
    "plaintext": true,
    "markdown": true
  },
  "github.copilot.editor.enableAutoCompletions": true,
  "gitlab.aiAssistedCodeSuggestions.enabled": true,
  "emojisense.languages": {
    "markdown": true,
    "json": true,
    "plaintext": {
      "markupCompletionsEnabled": false,
      "emojiDecoratorsEnabled": false
    },
    "scminput": true
  },
  "deno.enable": false,
  "npm.keybindingsChangedWarningShown": true,
  "playwright.reuseBrowser": false,
  "playwright.showTrace": true,
  "java.configuration.runtimes": [
  {
    "name": "JavaSE-17",
    "path": "/usr/lib/jvm/openjdk-bin-17",
